{
  "expo": {
    "name": "bjm-dev",
    "slug": "bjm-dev",
    "scheme": "bjm-dev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212122"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.bjm-dev"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#212122"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font", 
        {
          "fonts": [
            "./assets/fonts/LeagueSpartan-Black.ttf", 
            "./assets/fonts/LeagueSpartan-Bold.ttf", 
            "./assets/fonts/LeagueSpartan-ExtraBold.ttf", 
            "./assets/fonts/LeagueSpartan-ExtraLight.ttf",
            "./assets/fonts/LeagueSpartan-Light.ttf",
            "./assets/fonts/LeagueSpartan-Medium.ttf",
            "./assets/fonts/LeagueSpartan-Regular.ttf",
            "./assets/fonts/LeagueSpartan-SemiBold.ttf"
          ]
        }
      ]
    ]
  }
}
